<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="user1"
          class="org.wjj.ioc.ex5.assemble.User">
        <constructor-arg index="0" value="tom"></constructor-arg>
        <constructor-arg index="1" value="123456"></constructor-arg>
        <constructor-arg index="2" >
            <list>
                <value>"constructor value1"</value>
                <value>"constructor value2"</value>
            </list>
        </constructor-arg>
    </bean>

    <bean id="user2"
          class="org.wjj.ioc.ex5.assemble.User">
        <constructor-arg name="username" value="张三"></constructor-arg>
        <constructor-arg name="password" value="654321"></constructor-arg>
        <constructor-arg name="list" >
            <list>
                <value>"constructor value1"</value>
                <value>"constructor value2"</value>
            </list>
        </constructor-arg>
    </bean>

</beans>